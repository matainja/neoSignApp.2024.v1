<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:dither="false" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <corners android:radius="100dp" />
            <gradient android:endColor="#099F08" android:startColor="#099F08" />
            <size android:width="20dp" android:height="20dp" />

        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="false" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <corners android:radius="100dp" />
            <gradient android:endColor="#CACACA" android:startColor="#CACACA" />
            <size android:width="20dp" android:height="20dp" />

        </shape>
    </item>
</selector>
